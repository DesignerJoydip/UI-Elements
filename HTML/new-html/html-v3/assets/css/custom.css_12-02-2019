label.checkbox input[type="checkbox"]:checked + .chkIcon:before, label.radiobox input[type="radio"]:checked + .chkIcon:before { content: ''; width: 20px; height: 20px; position: absolute; top: -7px; left: 2px; background: url(../images/check.svg) no-repeat; }

label.radiobox .chkIcon i, label.checkbox .chkIcon i { position: absolute; font-size: .9em; line-height: 0; top: 55%; left: 10%; }

